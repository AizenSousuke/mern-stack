// From LTA DataMall
model BusStop {
    id          String   @id @default(auto()) @map("_id") @db.ObjectId
    busStopCode String   @unique
    roadName    String
    description String
    location    Json
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt

    BusStopSetting BusStopSetting[]
}
