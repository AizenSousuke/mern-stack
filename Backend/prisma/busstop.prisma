model BusStop {
    id          String   @id @default(auto()) @map("_id") @db.ObjectId
    busStopCode String   @unique
    roadName    String
    description String
    location    Json
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt

    settingSchemaGoingOut    SettingSchema? @relation("SettingsGoingOut", fields: [settingSchemaGoingOutId], references: [id])
    settingSchemaGoingHome   SettingSchema? @relation("SettingsGoingHome", fields: [settingSchemaGoingHomeId], references: [id])
    settingSchemaGoingOutId  String?        @db.ObjectId
    settingSchemaGoingHomeId String?        @db.ObjectId
}
